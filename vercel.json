{
    "builds":[
        {
            "src":"*.js","use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"/"
        }
    ],
    "headers":[

        {
            "origin":"https://e-commerce-app-frontend-pink.vercel.app",
            "Access-Control-Allow-Methods": ["POST","PUT","GET","DELETE","OPTIONS"],
            "Access-Control-Allow-Headers": "Content-Type"
        }
    ] 

}